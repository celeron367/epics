record(ai,"TOF:STATUS")
{
       field (DTYP, "stream")
       field (INP, "@smms.proto get_status MAIN_CONTROL")                                                    
       field (SCAN, ".5 second")                                                                    
} 

record(ai,"STATUS:RBV")
{      
       field (DTYP, "stream")
       #field(FTVL,"SHORT")
       field (INP, "@smms.proto read_status MAIN_CONTROL")     
       field (SCAN, "I/O Intr")
}


