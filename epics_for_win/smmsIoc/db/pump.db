###-----------------------------------------

record(ao,"PUMP:OPEN")
{
       field (DTYP, "stream")
       field (OUT, "@pump.proto pump_open PUMP")                                                          
} 

record(ao,"PUMP:CLOSE")
{
       field (DTYP, "stream")
       field (OUT, "@pump.proto pump_close PUMP")                                                                                                                     
} 


record(ao,"PUMP:GET:PARA")
{
       field (DTYP, "stream")
       field (OUT, "@pump.proto pump_para PUMP")                                                    
       field (SCAN, "1 second")                                                                    
} 

record(ao,"PUMP:GET:TEM")
{
       field (DTYP, "stream")
       field (OUT, "@pump.proto pump_tem PUMP")                                                    
       field (SCAN, "1 second")                                                                    
} 

record(ao,"PUMP:GET:SPEED")
{
       field (DTYP, "stream")
       field (OUT, "@pump.proto pump_speed PUMP")                                                    
       field (SCAN, "1 second")                                                                    
} 















