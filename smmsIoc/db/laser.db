record(ai,"LASER:ENERGY:RAW")
{
       field (DESC,"")  
       field (DTYP, "stream")
       field (INP,  "@laser.proto read_energy LASER")
       field (FLNK,"LASER:ENERGY:RBV")                                              
       field (SCAN, "1 second")                                                                    
} 

record(calc,"LASER:ENERGY:RBV")
{
       field (DESC,"")  
       field (INPA, "LASER:ENERGY:RAW")    
       field (INPB, "LASER:SET:TRANS NPP")
       field (CALC,"A/64000*250*B/100")                                                                                                                     
} 

