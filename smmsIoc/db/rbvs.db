#-----------------Gauge-----------------------------------------
record(ai,"GAU:1:MBI")
{
       field (DTYP,"Soft Channel")      
       field (FLNK,"GAU:2:RBV")                   
}

record(calc,"GAU:2:RBV")
{
       field(DESC,"")                                                             
       field(INPA,"GAU:1:MBI")
       field(CALC,"10^((A/65535*10-3.572)/1.286)")      
}  

record(ai,"GAU:2:MBI")
{
       field (DTYP,"Soft Channel") 
       field (FLNK,"GAU:1:RBV")                            
}

record(calc,"GAU:1:RBV")
{
       field(DESC,"")                                                             
       field(INPA,"GAU:2:MBI")
       field(CALC,"10^(((A+1500)/65535*10-3.572)/1.286)")      
}  

