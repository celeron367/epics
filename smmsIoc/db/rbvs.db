#----------------MOTOR------------------------------------------
record(ai,"MOTOR:X:MBI")
{
       field (DTYP,"Soft Channel")  
       field (FLNK,"MOTOR:X:RBV PP")                          
}

record(calc,"MOTOR:X:RBV")
{
       field(DESC,"calc x positon ")                                                             
       field(INPA,"MOTOR:X:MBI")
       field(CALC,"A/200")      
}  

